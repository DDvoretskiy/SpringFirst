<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="client" class="by.izone.spring.core.beans.Client">
        <constructor-arg value="1"/>
        <constructor-arg value="Ivan Petrov"/>
    </bean>

    <bean id="eventLogger" class="by.izone.spring.core.loggers.ConsoleEventLogger">

    </bean>

    <bean id="app" class="by.izone.spring.core.App">
        <constructor-arg ref="client"/>
        <constructor-arg ref="eventLogger"/>
    </bean>
</beans>

